{
	"Moderation": [
		{
			"t": ["ban"],
			"u": "/ban <user> <reason>",
			"d": "Ban a user from the server",
			"p": ["banMembers"]
		},
		{
			"t": ["kick"],
			"u": "/kick <user> <reason>",
			"d": "Kick a user from the server",
			"p": ["kickMembers"]
		},
		{
			"t": ["warn add"],
			"u": "/warn add <user> <reason>",
			"d": "Add a warning to a user",
			"p": ["manageMessages"]
		},
		{
			"t": ["warn delete"],
			"u": "/warn delete <warn id>",
			"d": "Remove a warning from a user",
			"p": ["manageMessages"]
		},
		{
			"t": ["warn list"],
			"u": "/warn list <user>",
			"d": "List of a user's warnings",
			"p": ["manageMessages"]
		}
	],
	"Utilities": [
		{
			"t": ["welcome channel"],
			"u": "/welcome channel <#channel>",
			"d": "The channel for the welcome message to be sent to",
			"p": ["manageServer"]
		},
		{
			"t": ["welcome message"],
			"u": "/welcome message <content>",
			"d": "The message that you want to be sent as in the welcome message",
			"p": ["manageServer"]
		},
		{
			"t": ["welcome options"],
			"u": "/welcome options",
			"d": "The message options that will be edited in the welcome message",
			"p": ["manageServer"]
		},
		{
			"t": ["modlog"],
			"u": "/modlog <#channel>",
			"d": "Set the mod-log channel for the bot",
			"p": ["manageServer"]
		},
		{
			"t": ["chatbot"],
			"u": "/chatbot <#channel>",
			"d": "Set the channel for the chatbot",
			"p": ["manageServer"]
		}
	],
	"Misc": [
		{
			"t": ["whois"],
			"u": "/whois [user]",
			"d": "Get Information of a user",
			"p": ["sendMessages"]
		},
		{
			"t": ["serverinfo"],
			"u": "/serverinfo",
			"d": "Get Information of the server",
			"p": ["sendMessages"]
		},
		{
			"t": ["meme"],
			"u": "/meme",
			"d": "Get a random meme!",
			"p": ["sendMessages"]
		}
	],
